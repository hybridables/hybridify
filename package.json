  "repository": "hybridables/hybridify",
  "author": "Charlike Mike Reagent <@tunnckoCore> (http://www.tunnckocore.tk)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "standard && node test.js"
  },
  "dependencies": {
    "handle-arguments": "^3.1.0",
    "lazy-cache": "^2.0.1",
    "letta": "^0.5.0",
    "sliced": "^1.0.1",
    "then-callback": "^1.0.3"
  },
  "devDependencies": {
    "mukla": "^0.4.1",
    "pre-commit": "*"
  },
  "files": [
    "index.js",
    "utils.js"
  ],
  "keywords": [
    "hybridify"
  ],
  "verb": {
